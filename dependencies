#! /bin/bash

current=$(pwd)
dir=$(mktemp -d -t cosy.XXXXXX)
cd ${dir}
  echo "Working in ${dir}"
  git clone https://github.com/ErnieE5/ee5_base64.git
  cp ee5_base64/ee5_base64.lua ${current}/src/
  git clone https://github.com/LuaDist/dkjson.git
  cp dkjson/dkjson.lua ${current}/src/
cd ${current}
rm -rf ${dir}
